{
  "name": "@folio/calendar",
  "version": "2.0.3",
  "description": "Opening hours",
  "repository": "folio-org/ui-calendar",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "main": "index.js",
  "stripes": {
    "type": "settings",
    "displayName": "ui-calendar.meta.title",
    "route": "/calendar",
    "hasSettings": true,
    "icons": [
      {
        "name": "app",
        "alt": "Institutional calendar",
        "title": "Calendar"
      }
    ],
    "okapiInterfaces": {
      "calendar": "1.0",
      "service-points": "2.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.calendar.enabled",
        "displayName": "UI: Calendar module is enabled"
      },
      {
        "permissionName": "ui-calendar.all",
        "displayName": "Calendar: All permissions",
        "description": "Entire set of permissions needed to use the calendar module",
        "subPermissions": [
          "calendar.collection.all",
          "ui-calendar.edit",
          "ui-calendar.create",
          "ui-calendar.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.view",
        "displayName": "Calendar: View calendar events",
        "description": "Can view calendar events",
        "subPermissions": [
          "calendar.collection.get",
          "module.calendar.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.edit",
        "displayName": "Calendar: Edit calendar events",
        "description": "Can edit calendar events",
        "subPermissions": [
          "ui-calendar.view",
          "calendar.collection.update"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.create",
        "displayName": "Calendar: Create calendar events",
        "description": "Can create calendar events",
        "subPermissions": [
          "ui-calendar.view",
          "calendar.collection.add"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.delete",
        "displayName": "Calendar: Delete calendar events",
        "description": "Can remove calendar events",
        "subPermissions": [
          "ui-calendar.view",
          "calendar.collection.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "settings.calendar.enabled",
        "displayName": "Settings (Calendar): display calendar settings",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  },
  "scripts": {
    "lint": "eslint *.js settings ",
    "test": "echo 'placeholder. no tests implemented'"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^3.2.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^4.8.0",
    "webpack": "1.11.0"
  },
  "dependencies": {
    "@folio/react-big-calendar": "^1.0.0",
    "@folio/react-intl-safe-html": "^1.0.2",
    "@folio/stripes-components": "^3.1.0",
    "@folio/stripes-form": "^1.0.0",
    "@folio/stripes-smart-components": "^1.8.0",
    "css-loader": "^0.28.11",
    "dateformat": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.1",
    "proptypes": "^1.1.0",
    "query-string": "^6.1.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "redux-form": "^7.0.3",
    "style-loader": "^0.21.0",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "@folio/stripes-connect": "^3.2.0",
    "@folio/stripes-core": "^2.13.0",
    "react": "*",
    "react-bootstrap": "0.32.1",
    "react-dom": "^16.3.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.0.0"
  }
}
