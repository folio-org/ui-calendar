{
  "name": "@folio/calendar",
  "version": "2.1.3",
  "description": "Opening hours",
  "repository": "folio-org/ui-calendar",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "main": "index.js",
  "stripes": {
    "type": "settings",
    "displayName": "ui-calendar.meta.title",
    "route": "/calendar",
    "hasSettings": true,
    "icons": [
      {
        "name": "app",
        "alt": "Institutional calendar",
        "title": "Calendar"
      }
    ],
    "okapiInterfaces": {
      "calendar": "2.0",
      "service-points": "3.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.calendar.enabled",
        "displayName": "UI: Calendar module is enabled"
      },
      {
        "permissionName": "ui-calendar.all",
        "displayName": "Calendar: All permissions",
        "description": "Entire set of permissions needed to use the calendar module",
        "subPermissions": [
          "inventory-storage.service-points.collection.get",
          "calendar.collection.all",
          "ui-calendar.edit",
          "ui-calendar.create",
          "ui-calendar.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.view",
        "displayName": "Calendar: View calendar events",
        "description": "Can view calendar events",
        "subPermissions": [
          "calendar.collection.get",
          "module.calendar.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.edit",
        "displayName": "Calendar: Edit calendar events",
        "description": "Can edit calendar events",
        "subPermissions": [
          "ui-calendar.view",
          "calendar.collection.update"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.create",
        "displayName": "Calendar: Create calendar events",
        "description": "Can create calendar events",
        "subPermissions": [
          "ui-calendar.view",
          "calendar.collection.add"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-calendar.delete",
        "displayName": "Calendar: Delete calendar events",
        "description": "Can remove calendar events",
        "subPermissions": [
          "ui-calendar.view",
          "calendar.collection.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "settings.calendar.enabled",
        "displayName": "Settings (Calendar): display calendar settings",
        "subPermissions": [
          "inventory-storage.service-points.collection.get",
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  },
  "scripts": {
    "start": "stripes serve",
    "lint": "eslint .",
    "test": "stripes test karma"
  },
  "devDependencies": {
    "@bigtest/convergence": "^1.0.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-junit-formatter": "^0.2.1",
    "@bigtest/interactor": "^0.9.2",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.2",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^2.4.0",
    "@folio/stripes-cli": "^1.8.0",
    "@folio/stripes-core": "^3.4.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.5.0",
    "mocha": "^6.1.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "@folio/react-big-calendar": "^2.0.0",
    "@folio/react-intl-safe-html": "^1.0.2",
    "css-loader": "^0.28.11",
    "dateformat": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.1",
    "proptypes": "^1.1.0",
    "query-string": "^6.1.0",
    "randomcolor": "^0.5.3",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-intl": "^2.7.0",
    "redux-form": "^7.0.3",
    "style-loader": "^0.21.0",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^2.4.0",
    "react": "*",
    "react-bootstrap": "0.32.1",
    "react-dom": "^16.3.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.0.0"
  }
}
