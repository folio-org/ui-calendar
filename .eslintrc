{
  "extends": ["@folio/eslint-config-stripes", "plugin:import/typescript"],
  "ignorePatterns": ["src/typings/stripes/components/**/*"],
  "rules": {
    "import/no-named-as-default": "off"
  },
  "overrides": [
    {
      "files": ["src/test/**/*", "src/**/*.test.ts", "src/**/*.test.tsx"],
      "env": { "jest": true }
    }
  ]
}
